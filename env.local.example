# Local Development Environment Variables
# Copy to .env and fill in only R2 credentials

# Redis - LOCAL (works out of the box with docker-compose)
UPSTASH_REDIS_REST_URL=http://localhost:8079
UPSTASH_REDIS_REST_TOKEN=local_dev_token

# R2 Configuration - FILL THESE IN
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_INPUT_BUCKET_NAME=tcoder-input
R2_OUTPUT_BUCKET_NAME=tcoder-output

# Fly.io - FILL THESE IN (only needed if spawning real machines)
FLY_API_TOKEN=
FLY_APP_NAME=fly-tcoder-ffmpeg-worker-31657fa
FLY_REGION=fra
